{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "right",
      "segments": [
        {
         "background": "#9A348E",
          "foreground": "#ffffff",
          "leading_diamond": "",
          "style": "diamond",
          "template": " {{ .UserName }} ",
          "trailing_diamond": "",
          "type": "session"
        },
        {
          "background": "#DA627D",
          "foreground": "#ffffff",
          "leading_diamond": "",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": " \ue5ff {{ .Path }} ",
          "trailing_diamond": "",
          "type": "path"
        },
        {
          "background": "#FCA17D",
          "foreground": "#ffffff",
          "leading_diamond": "",
          "properties": {
            "branch_icon": ""
          },
          "style": "diamond",
          "template": " {{ .HEAD }} ",
          "trailing_diamond": "",
          "type": "git"
        },
        {
          "type": "aws",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",

          "background": "#86BBD8",
          "foreground": "#ffffff",
          "template": "  îž­ {{.Profile}}{{if .Region}}@{{.Region}}{{end}}  "
        },
        {
          "background": "#33658A",
          "foreground": "#ffffff",
          "properties": {
            "time_format": "15:04"
          },
          "style": "diamond",
          "template": "  \u2665 {{ .CurrentDate | date .Format }}  ",
          "leading_diamond": "",
          "trailing_diamond": "",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#7FD5EA",
          "leading_diamond": "",
          "style": "diamond",
          "template": " {{ .Path }}\u276f",
          "trailing_diamond": "",
          "type": "path"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
